BEGIN
 IF CHECKMAP C= MAP
   IF CHECKPOS = POS
     IF CHECKMAP = MAP
       IF CHRCKSQL ISLAND = TRUE
         TELEPORT TO ISLAND = ISLAND
       ELSEIF CHECKSQL ISLAND = FALSE
         TELEPORT TO ISLAND = ISLAND
   ELSEIF CHECKPOS = POS
       IF CHRCKMAP = MAP
         CHANGEMAP = MAP
 ELSEIF CHECKMAP C= MAP
     IF CHRCKPOS = POS
       IF CHECKMAP = MAP
         TELEPORT TO ISLAND = ISLAND
         SQLSET SQL = SQL
END
